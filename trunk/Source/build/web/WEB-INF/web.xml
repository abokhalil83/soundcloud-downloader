<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>tt</servlet-name>
        <servlet-class>parser.controller.tt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>download</servlet-name>
        <servlet-class>parser.controller.download</servlet-class>
        <init-param>
            <param-name>propFile</param-name>
            <param-value>/WEB-INF/properties/jdbc.properties</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>parser.controller.test</servlet-class>
        <init-param>
            <param-name>propFile</param-name>
            <param-value>/WEB-INF/properties/jdbc.properties</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>tt</servlet-name>
        <url-pattern>/tt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>download</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
